    <!-- ======= -->
    <!-- <h2><a href="/patient/logout">Logout</a></h2> -->
    <!-- <h2><a href="/patient/medicine">List Medicine</a></h2> -->
    <% if (patient.status === `Pending`) { %>
        <div class="flex justify-between mt-5 text-center">
            <h2 class="text-4xl px-5 py-2.5 mb-0">List Doctor</h2>
    
            <form action="" class="text-right">
                <!-- class="block w-full p-2 ps-10 text-sm text-green-900 border border-green-300 rounded-lg bg-green-50 focus:ring-blue-500 focus:border-blue-500 dark:bg-green-700 dark:border-green-600 dark:placeholder-green-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500" -->
                <select name="filter" id="" class="rounded-lg px-5 py-2.5" style="width: 200px;">
                        <option value="" selected disabled>Choose...</option>
                        <option value="Penyakit Dalam">Penyakit Dalam</option>
                        <option value="Umum">Umum</option>
                </select>
                <button type="submit" class="focus:outline-none text-white bg-yellow-400 hover:bg-yellow-500 focus:ring-4 focus:ring-yellow-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:focus:ring-yellow-900">Filter</button>
            </form>

        </div>
        <div class="relative overflow-x-auto shadow-md sm:rounded-lg">
            <table class="w-full text-sm text-left rtl:text-right text-green-500 dark:text-gray-700">
                <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-300 dark:text-gray-700">
                    <tr>
                        <th scope="col" class="px-6 py-3">Name</th>
                        <th scope="col" class="px-6 py-3">Specialist</th>
                        <th scope="col" class="px-6 py-3">Work of Experience (inYear)</th>
                        <th scope="col" class="px-6 py-3">Hospital</th>
                        <th scope="col" class="px-6 py-3">Price</th>
                        <th scope="col" class="px-6 py-3">Action</th>
                    </tr>
                </thead>
                <tbody>
                    <% data.forEach(el => { %>
                     <tr class="odd:bg-white odd:dark:bg-gray-100 even:bg-gray-50 even:dark:bg-gray-400 border-b dark:border-gray-700">
                        <td class="px-6 py-4"><%= el.title %> <%= el.name %></td>
                        <td class="px-6 py-4"><%= el.specialist %></td>
                        <td class="px-6 py-4"><%= el.exp %></td>
                        <td class="px-6 py-4"><%= el.hospital %></td>
                        <td class="px-6 py-4"><%= rupiah(el.price) %></td>
                        <td class="px-6 py-4"><a href="/patient/<%= el.id %>"><button class="focus:outline-none text-white bg-red-700 hover:bg-red-800 focus:ring-4 focus:ring-red-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-red-600 dark:hover:bg-red-700 dark:focus:ring-red-900">Start Consultation</button></a></td>
                     </tr>
                    <% }) %>
                </tbody>
            </table>

        </div>
    <% } %>

    <!-- on Wait -->
    

