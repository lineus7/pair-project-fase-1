    <!-- ==== -->
    <% if (patient.status === `Waiting`) { %>
        <h1 class="text-center mx-10 my-5">Anda sedang menunggu konfirmasi dari <% data.forEach(el => { %>
               <% if (el.id === patient.DoctorId) { %>
                <%= el.title+` `+el.name %>
               <% } %>
        <% }) %></h1>
        <a href="/patient/<%= patient.DoctorId %>/cancel"><button class="focus:outline-none text-white bg-red-700 hover:bg-red-800 focus:ring-4 focus:ring-red-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-red-600 dark:hover:bg-red-700 dark:focus:ring-red-900">Batalkan Konsultasi</button></a>
       <% } %>
       <% if (patient.status === `Accepted`) { %>
           <h1 class="text-center text-4xl">Mulai Berdiskusi dengan Dokter Anda</h1>
           <a href="/patient/<%= patient.DoctorId %>/cancel"><button class="focus:outline-none text-white bg-red-700 hover:bg-red-800 focus:ring-4 focus:ring-red-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-red-600 dark:hover:bg-red-700 dark:focus:ring-red-900">Batalkan Konsultasi</button></a>
           <br>
           <a href="/patient/<%= patient.DoctorId %>/chat" class=" mx-15 my-5 focus:outline-none text-white bg-red-700 hover:bg-red-800 focus:ring-4 focus:ring-red-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-red-600 dark:hover:bg-red-700 dark:focus:ring-red-900"><button class="mx-5">Mulai Obrolan</button></a>
       <% } %>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/flowbite/2.2.0/flowbite.min.js"></script>
